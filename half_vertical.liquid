        {% elsif dest_lower == 'rich' or dest_lower contains 'richmond' %}
          {% assign destination_name = 'Richmond' %}
        {% elsif dest_lower == 'rock' or dest_lower contains 'rockridge' %}
          {% assign destination_name = 'Rockridge' %}
        {% elsif dest_lower == 'sbrn' or dest_lower contains 'san bruno' %}
