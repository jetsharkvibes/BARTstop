        {% elsif dest_lower == 'oakl' or dest_lower contains 'oakland' and dest_lower contains 'airport' %}
          {% assign destination_name = 'OAK Airport' %}
        {% elsif dest_lower == 'orin' or dest_lower contains 'orinda' %}
          {% assign destination_name = 'Orinda' %}
        {% elsif dest_lower == 'pctr' or dest_lower contains 'pittsburg center' %}
          {% assign destination_name = 'Pittsburg Center' %}
        {% elsif dest_lower == 'pitt' or dest_lower contains 'pittsburg' or dest_lower contains 'bay point' %}
