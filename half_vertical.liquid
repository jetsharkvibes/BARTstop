          {% assign destination_name = 'Daly City' %}
        {% elsif dest_lower == 'dbrk' or dest_lower contains 'downtown berkeley' %}
          {% assign destination_name = 'Downtown Berkeley' %}
        {% elsif dest_lower == 'dubl' or dest_lower == 'wdub' or dest_lower contains 'dublin' or dest_lower contains 'pleasanton' %}
          {% assign destination_name = 'Dublin / Pleasanton' %}
        {% elsif dest_lower == 'deln' or dest_lower contains 'el cerrito del norte' %}
          {% assign destination_name = 'El Cerrito del Norte' %}
        {% elsif dest_lower == 'plza' or dest_lower contains 'el cerrito plaza' %}
          {% assign destination_name = 'El Cerrito Plaza' %}
        {% elsif dest_lower == 'embr' or dest_lower contains 'embarcadero' %}
          {% assign destination_name = 'Embarcadero' %}
        {% elsif dest_lower == 'warm' or dest_lower contains 'fremont' and dest_lower contains 'warm' or dest_lower contains 'warm springs' %}
          {% assign destination_name = 'Warm Springs / South Fremont' %}
        {% elsif dest_lower == 'frmt' or dest_lower contains 'fremont' %}
          {% assign destination_name = 'Fremont' %}
        {% elsif dest_lower == 'ftvl' or dest_lower contains 'fruitvale' %}
