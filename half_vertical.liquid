        {% elsif dest_lower == 'bery' or dest_lower contains 'berryessa' or dest_lower contains 'north san jose' %}
          {% assign destination_name = 'Berryessa / North San Jose' %}
        {% elsif dest_lower == 'cast' or dest_lower contains 'castro valley' %}
          {% assign destination_name = 'Castro Valley' %}
        {% elsif dest_lower == 'civc' or dest_lower contains 'civic' %}
          {% assign destination_name = 'Civic Center / UN Plaza' %}
        {% elsif dest_lower == 'cols' or dest_lower contains 'coliseum' %}
          {% assign destination_name = 'Coliseum' %}
        {% elsif dest_lower == 'colm' or dest_lower contains 'colma' %}
          {% assign destination_name = 'Colma' %}
        {% elsif dest_lower == 'ncon' or dest_lower contains 'concord' and dest_lower contains 'north' %}
          {% assign destination_name = 'North Concord / Martinez' %}
        {% elsif dest_lower == 'conc' or dest_lower contains 'concord' %}
